<div class="container">
	<p id="notice"><%= notice %></p>
	<div class="row">
		<div class="col-md-12">
			<div class="col-md-10">
				<h2><%= @admin_post.title %></h2>
				<p><%= @admin_post.description %></p>
			</div>
			<div class="col-md-2">
				<%= link_to 'Edit Post', edit_admin_post_path(@admin_post), class: "btn btn-warning" %>
			</div>
		</div>
	</div>
	<div class="row">
		<div class="col-lg-12">
	        <div class="ibox">
	            <div class="ibox-content">
	                <table class="table table-striped">
	                	<thead>
	                    <tr>
	                        <th>Listing Company</th>
	                        <th>URL</th>
	                        <th>Price</th>
	                    </tr>
	                    </thead>
	                    <tbody id="device-index">
	                    	<% @admin_post_details.each do |adp| %>
	                    		<tr>
	                    			<td><%= adp.listing_company %></td>
	                    			<td><%= adp.url %></td>
	                    			<td>$<%= adp.price %></td>
	                    		</tr>
                    		<% end %>
                    	</tbody>
	               	</table>
	            </div>
	        </div>
	    </div>
	</div>
</div>
